{
  "manifest_version": 2,
  "name": "theBlurer",
  "description": "Remove paywall, blur e randomizacao de texto de um site de provas a√≠...",
  "version": "1.4.2",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["deblur.js"]
      }
  ],
  "web_accessible_resources": [
    "arquivo.js"
  ]
}
